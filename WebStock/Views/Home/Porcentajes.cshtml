
@{
    Layout = null;
}

@model MisFinanzas.Models.Porcentaje
 
<div class="containter">
    <div class="row">
        <form class="form-horizontal" name="commentform" method="post" action="/Home/Save">
            <div class="col-xs-12">
                <div class="col-xs-12 col-xs-offset-2 form-group">
                    <label for="Obligatorio">% Obligatorio</label>
                    @Html.EditorFor(model => model.Obligatorio, new { htmlAttributes = new { @class = "form-control" } })
                </div> 
                <div class="col-xs-12 form-group">
                    <label for="Deseoso">% Deseoso</label>
                    @Html.EditorFor(model => model.Deseoso, new { htmlAttributes = new { @class = "form-control" } })
                </div> 
                <div class="col-xs-12 form-group">
                    <label for="inversion">% Inversión</label>
                    @Html.EditorFor(model => model.Inversion, new { htmlAttributes = new { @class = "form-control" } })
                </div> 
                <div class="col-xs-12 form-group">
                    <button type="submit" class="btn btn-success" style="float: right;">
                        Guardar
                        <span class="fa fa-floppy-o"></span>
                    </button>
                </div>
            </div>
        </form>
    </div>
</div>
 
